<?xml version="1.0" encoding="UTF-8"?>
<jeu:Jeu xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:jeu="http://www.n7.fr/Jeu"
    name="JeuEnigme">
  <jeuelement
      xsi:type="jeu:Connaissance"
      name="Reussite"
      Visibilite="Invisible"/>
  <jeuelement
      xsi:type="jeu:Objet"
      name="tentative"
      taille="1"
      Visibilite="Invisible"/>
  <jeuelement
      xsi:type="jeu:Personne"
      name="Sphinx">
    <Interaction
        question="Question"
        joueur="//@jeuelement.6"
        personne="//@jeuelement.2">
      <choix
          name="Choix1">
        <action_du_choix
            Victime="//@jeuelement.6"
            Attribution_connaissance="//@jeuelement.0"/>
      </choix>
      <choix
          name="Choix2">
        <action_du_choix
            Victime="//@jeuelement.6"
            Consommation_objet="//@jeuelement.1"/>
      </choix>
    </Interaction>
    <conditionvisibilite
        Contrainte_connaissance="true"
        condition_connaissance="//@jeuelement.0"/>
  </jeuelement>
  <jeuelement
      xsi:type="jeu:Endroit"
      name="Enigme"
      personne="//@jeuelement.2">
    <lieu xsi:type="jeu:Debut">
      <Chemin_suivant
          name="DebutversSucces"
          Lieu_precedent="//@jeuelement.3"
          Lieu_suivant="//@jeuelement.4">
        <condition_ouverture
            estVerifie="true"/>
      </Chemin_suivant>
      <Chemin_suivant
          name="DebutversEchec"
          Lieu_precedent="//@jeuelement.3"
          Lieu_suivant="//@jeuelement.5">
        <condition_ouverture
            estVerifie="true"/>
      </Chemin_suivant>
    </lieu>
  </jeuelement>
  <jeuelement
      xsi:type="jeu:Endroit"
      name="Succes"
      ouvertOUferme="Ferme">
    <lieu xsi:type="jeu:Fin">
      <Chemin_entrants
          name="ebutversSucces"
          Lieu_precedent="//@jeuelement.3"
          Lieu_suivant="//@jeuelement.4"/>
    </lieu>
  </jeuelement>
  <jeuelement
      xsi:type="jeu:Endroit"
      name="Echec"
      ouvertOUferme="Ferme">
    <lieu xsi:type="jeu:Fin">
      <Chemin_entrants
          name="DebutversEchec"
          Lieu_precedent="//@jeuelement.3"
          Lieu_suivant="//@jeuelement.5"/>
    </lieu>
  </jeuelement>
  <jeuelement
      xsi:type="jeu:Joueur"
      name="explorateur"
      taille="10"
      lieu="//@jeuelement.3">
    <allocateurObjetJoueur
        name="alocateurTentative"
        nboccurence="3"
        objet="//@jeuelement.1"
        proprietaire="//@jeuelement.6"/>
  </jeuelement>
</jeu:Jeu>
