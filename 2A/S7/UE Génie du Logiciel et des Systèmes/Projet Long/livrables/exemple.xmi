<?xml version="1.0" encoding="UTF-8"?>
<JEU:Jeu xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:JEU="http://www.n7.fr/JEU"
    xsi:schemaLocation="http://www.n7.fr/JEU Jeu_v2.ecore"
    name="exemple">
  <jeuelement
      xsi:type="JEU:Debut"
      name="Depart"
      Chemin_suivant="//@jeuelement.1"/>
  <jeuelement
      xsi:type="JEU:Chemin"
      name="Debut2Sphynx"
      Lieu_precedent="//@jeuelement.0"
      Lieu_suivant="//@jeuelement.2"/>
  <jeuelement
      xsi:type="JEU:Lieu"
      name="DomaineDuSphynx"
      Chemin_sortants="//@jeuelement.3 //@jeuelement.6"
      Chemin_entrants="//@jeuelement.1"
      Interactions="//@jeuelement.8"/>
  <jeuelement
      xsi:type="JEU:Chemin"
      Visibilite="invisible"
      name="Sphynx2Succes"
      Lieu_precedent="//@jeuelement.2"
      Lieu_suivant="//@jeuelement.4">
    <Conditions_visibilite
        condition_connaissance="//@jeuelement.11"
        Contrainte_connaissance="true"/>
  </jeuelement>
  <jeuelement
      xsi:type="JEU:Fin"
      name="Succes"
      Chemin_entrants="//@jeuelement.3"/>
  <jeuelement
      xsi:type="JEU:Fin"
      name="Echec"
      Chemin_entrants="//@jeuelement.6"/>
  <jeuelement
      xsi:type="JEU:Chemin"
      name="Sphynx2Echec"
      Lieu_precedent="//@jeuelement.2"
      Lieu_suivant="//@jeuelement.5">
    <Conditions_visibilite
        condition_objet="//@jeuelement.7/@allocateurObjetJoueur.0"/>
  </jeuelement>
  <jeuelement
      xsi:type="JEU:Joueur"
      name="EquipeB4"
      taille="4">
    <allocateurObjetJoueur
        name="NombreDeTentative"
        nboccurence="3"
        objet="//@jeuelement.10"
        proprietaire="//@jeuelement.7"/>
  </jeuelement>
  <jeuelement
      xsi:type="JEU:Personne"
      Visibilite="invisible"
      name="Sphynx"
      Interaction="//@jeuelement.9">
    <Conditions_visibilite
        condition_objet="//@jeuelement.7/@allocateurObjetJoueur.0"
        condition_connaissance="//@jeuelement.11"
        nb_occurence_objet_min="1"
        nb_occurence_objet_max="-1"/>
  </jeuelement>
  <jeuelement
      xsi:type="JEU:Interaction"
      joueur="//@jeuelement.7"
      personne="//@jeuelement.8"
      question="2+2?">
    <choix name="3">
      <action_du_choix
          Victime="//@jeuelement.7"
          Consommation_objet="//@jeuelement.10"/>
    </choix>
    <choix name="4">
      <action_du_choix
          Victime="//@jeuelement.7"
          Attribution_connaissance="//@jeuelement.11"/>
    </choix>
  </jeuelement>
  <jeuelement
      xsi:type="JEU:Objet"
      name="Tentative"
      taille="1"/>
  <jeuelement
      xsi:type="JEU:Connaissance"
      name="Reussite"/>
</JEU:Jeu>
